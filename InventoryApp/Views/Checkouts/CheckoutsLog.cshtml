@model IEnumerable<InventoryApp.Models.Checkout>

@{
    ViewData["Title"] = "Inventory Log";
}

<h2>Inventory Log</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InventoryItem.Serial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InventoryItem.Item.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Start)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.End)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InventoryItem.Serial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InventoryItem.Item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Start)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.End)
            </td>
        </tr>
}
    </tbody>
</table>
